    {
      "name": "max",
      "displayName": "Max",
      "type": "number",
      "default": 20,
      "placeholder":  20
    },
    {
      "name": "color",
      "displayName": "Color",
      "type": "color",
      "default": "#00A898"
    },
    {
      "name": "secondaryColor",
      "displayName": "Secondary Color",
      "type": "color",
      "default": "#57DAC9"
    }
