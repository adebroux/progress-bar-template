{
  "displayName": "Progress Bar",
  "defaultWidth": 160,
  "defaultHeight": 24,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "variable",
      "displayName": "What is this progress bar tracking?",
      "type": "number",
      "role":  "autoSaveInput",
      "default": 5,
